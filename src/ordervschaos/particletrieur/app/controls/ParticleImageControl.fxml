<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<fx:root style="-fx-border-color: grey; -fx-border-width: 1;" styleClass="control-shadow-0p2" stylesheets="@../resources/styles/style.css" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox spacing="3.0" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0">
            <children>
                <Label fx:id="labelId" text="Index" />
                <Label fx:id="labelClassification" text="Classification" />
                <Label fx:id="labelScore" text="Score" />
            <StackPane>
               <children>
                      <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </StackPane>
            </children>
        </VBox>
    </children>
</fx:root>
